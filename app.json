{
  "expo": {
    "name": "strom_info",
    "slug": "strom-info",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splashScreen.png",
      "resizeMode": "cover",
      "backgroundColor": "#384766"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.flyinglawnmower.strominfo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "",
        "backgroundColor": "#384766"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.strom_app.strom_app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": ["strom-info-scheme"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bfc738a2-bb3e-4425-91d1-67b20ef18a80"
      }
    },
    "owner": "flyinglawnmower"
  }
}
